FoamFile
{
    version     5.0;
    format      ascii;
    class       volScalarField;
    object      Cpart2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inlet
      {
        type fixedValue;
        value uniform 1;
      }
    outlet
      {
	type fixedValue;
        value uniform 1;
      }
    walls
      {
	type zeroGradient;
      }
    frontAndBack
      {
        type empty;
      }
}

